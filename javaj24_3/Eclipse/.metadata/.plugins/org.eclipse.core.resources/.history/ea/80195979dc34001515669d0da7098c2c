package ch4;
import java.util.Date;
public class emploee {
	
	int  empid;
	String  empname;
   
  Date1 d1;
    
    
    emploee( int  empid,String  empname,Date1 d1)
    {
    	super();
    	this.empid=empid;
    	this.empname=empname;
    	this.d1=d1;
    }

   public  static  void  main(String  args[]) 
   {
	   Date1 d1=new Date1(18,4,1994);
	  emploee e= new  emploee(1,"heena",new  Date1(28,7,1015));
	 java.util. Date  d= new java.util.Date();
	 System.out.println(e.empid);
	 System.out.println(e.empname);
	 System.out.println(e.d1);
	int currentyear=d.getYear();
	currentyear=2000+(currentyear-100);
    int  age= currentyear-e.d1.yy;
    System.out.println("age  of  employee  is" +age);
  
	
	 
	
	 
	 
   }
    
}
