package ch7;
import java.util.Scanner;

public class DivisionDemo 
{
 int c,a,b;
	
public  static  void division()
{  
	Scanner  s=new Scanner(System.in);
	System.out.println("enter  the  two  numbers  u  want  to    divide");
	int  a=s.nextInt();
	int  b=s.nextInt();
	  try
	  {
		 c=b/a;
		 
	  }
  catch( ArithmeticException  ae)
  {
	  
  }
}
