StringBuffer  s1=new  StringBuffer("abc");
StringBuilder  b3=  new  StringBuilder(16);


if(s==s2)
{
System.out.println("equals");
}   
else 
	System.out.println("not  equals");
if(s.equals(s2))
		{
System.out.println("equals");
}   
if(s.equals(s1))
{
System.out.println("not  Equals");
}
else 
	System.out.println("not  equals");

}
}
