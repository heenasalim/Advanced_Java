package ch6;

public class Wrapper {
    int  x=30;
   Integer i=  new  Integer(x);
   public  void  show()
   {
 System.out.println (" primitive  data  type  to  wrapper"+x);
      
}
   //convert  wrapper   to  primitive 
   int  y=i.intvalue();
   System.out.println("wrapper  to  primitive:"+y);
}
