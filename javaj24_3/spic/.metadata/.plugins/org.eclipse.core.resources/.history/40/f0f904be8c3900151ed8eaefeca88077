package ch6;

public abstract class staticinner {
	int  accid;
	String accname;
    int  balance;
	static  class  Locker
	  {
		 int  locid;
		Locker(int  locid)
		{
		this.locid=locid;
		
		}
		public  static void  show()
		{
		System.out.println("lcid=="+locid+"acdcid"+accid+"account  name"+accname+"balance"+balance);
		}
		
	  }

}
