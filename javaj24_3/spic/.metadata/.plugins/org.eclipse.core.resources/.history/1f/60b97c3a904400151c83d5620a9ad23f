package balls;

public class Synchrinizedtransaction   extends   Thread {

	Thread  t1;
	Thread   t2;
	Synchrinizedtransaction()
	{
		t1=new  Thread(this,"thread1");
		t2=new  Thread(this,"thread  2");
		t1.start();
		t2.start();
	}
	
	 public  void  run()
	{
		if(Thread.currentThread()==t1)
		{
			
		deposite();
		
			
			
			
		}
	}
	
	
	
	
	
	
}
