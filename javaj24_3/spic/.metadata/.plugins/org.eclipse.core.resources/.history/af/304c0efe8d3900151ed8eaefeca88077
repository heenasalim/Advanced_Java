package ch6;

public abstract class staticinner {
	   static int  accid;
	   static String accname;
       static int  balance;
       
	     static  class  Locker
	  {
		static int  locid;
		Locker(int  locid,int accid,String  accname,int  balance )
		{
		Locker.locid=locid;
		accid=accid;
		accname=accname;
		balance=balance;
		}
		public   void  show()
		{
		System.out.println("lcid=="+locid+"acdcid"+accid+"account  name"+accname+"balance"+balance);
		}
		
	  }

}
