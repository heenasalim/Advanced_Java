package  swings;
import  javax.swing.*;
public class SwinDemo {
      JFrame  f;
      JPanel  p;
      JTextField  tf;
      JPasswordField  tp;
      JLabel  l1,l2
      JButton  b1,b2,b3;
      


SwinDemo()
{
	f=new JFrame("HEENAS FIRST  JAVA  APPLICATION ");
	p= new JPanel();
	tf=new JTextField(56);
	tp=new JPasswordField(56);
	l1=new JLabel("USERNAME");
	l2=new JLabel("PASSWORD");
  b1= new JButton("OK");
  b2=  new JButton("CANCEL");
  b3=  new JButton("EXIT");
  p.add(l1);
  p.add(tf);
  p.add(l2);
  p.add(tp);
  
  f.add(p);
  
}
}
