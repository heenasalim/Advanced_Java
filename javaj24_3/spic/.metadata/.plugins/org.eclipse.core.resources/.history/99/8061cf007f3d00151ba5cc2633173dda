package ch7

//java.lang.annotation.documented;
//java.lang.annotation.inherited;



public class Employee12 {
//	@Employee12(msg="  simple emplolyee  parameters   empid  empname,sal");
	int  empid;  double  sal;
	  String  empname;
	@Override
	public String toString() {
		return "Employee12 [empid=" + empid + ", sal=" + sal + ", empname="
				+ empname + "]";
	}
	//@Employee12(msg="emplolyee  const  with  empid  empname,sal");
	public Employee12(int empid, double sal, String empname) {
		
		super();
		this.empid = empid;
		this.sal = sal;
		this.empname = empname;
	}
	
	  
	  

}
