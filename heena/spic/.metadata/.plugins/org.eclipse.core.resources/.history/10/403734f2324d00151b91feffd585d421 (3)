  Connection  con=null;
	  try
	  {
		  Class.forName("oracle.jdbc.driver.OracleDriver");
		    System.out.println("got  connection");
		     con=DriverManager.getConnection("jdbc:Oracle:thin:@Oracle-server:1521:orcl","java","java") 
		  
	  }
