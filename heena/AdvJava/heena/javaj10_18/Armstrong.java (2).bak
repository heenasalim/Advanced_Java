class  Armstrong
{
	int no,sum,rem,n,c,a;
	Armstrong(int a)
	{
		no=a;
		c=no;
	}

	public void calAmstrong()
	{
		//System.out.println("enter the 3 digit no"+a);
	
		while(n>0)
		{
	rem=n%10;
	sum=sum+(rem*rem*rem);
	n=n/10;
		}
		if(sum==a)
		{
			System.out.println("no  is  armstrong:  "+c);
		}
		else
		{
			System.out.println("no  is not armstrong:  "+c);
		}

	}
	public static void main(String[] args) 
	{
		 Armstrong s =new Armstrong(153);
		  s.calAmstrong();
