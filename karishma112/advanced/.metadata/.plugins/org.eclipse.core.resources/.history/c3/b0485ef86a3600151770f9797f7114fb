
public class AccountML {
	public void display()
	{
		public class inner
		{
			public void show()
			{
				System.out.println("inside inner class");
			}
		}
		inner i=new inner();
		i.show();
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		AccountML m=new AccountML();
		m.display();

	}

}
