package javaIO;

import javax.swing.*;

public class Serialization extends JFrame {
JFrame f;
JLabel l1,l2,l3;
JTextField t1,t2,t3;
JPanel p;
JButton b1,b2,b3;

	public Serialization() {
		f=new JFrame();
		p=new JPanel();
		l1=new JLabel("RollNumber");
		l2=new JLabel("Name");
		l3=new JLabel("Marks");
		t1=new JTextField();
		t2=new JTextField();
		t3=new JTextField();
		b1=new JButton("Clear");
		b2=new JButton("Serialize");
		b3=new JButton("Deserialize");
		f.add(p);
		p.add(l1);
		p.add(l2);
		p.add(l3);
		p.add(t1);
		p.add(t2);
		p.add(t3);
		p.add(b1);
		p.add(b2);
		p.add(b3);
		f.setVisible(true);
		f.setSize(400,400);
		
		
	}

	public static void main(String[] args) {
		
		Serialization s=new Serialization();
		

	}

}
