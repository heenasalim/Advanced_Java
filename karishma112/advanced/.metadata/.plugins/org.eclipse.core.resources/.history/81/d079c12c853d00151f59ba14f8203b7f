package swingPK;
import javax.swing.*;

public class Login {
   
	JFrame f;
	JPanel p;
	JLabel l1,l2;
	JTextField t1,t2;
	JButton b1,b2,b3;
	Login()
	{
		f=new JFrame("LOGIN");
		p=new JPanel();
		l1=new JLabel("UserName");
		l2=new JLabel("PassWord");
		t1=new JTextField();
		t2=new JTextField();
		b1=new JButton("Submit");
		b2=new JButton("Clear");
		b3=new JButton("Cancel");
		p.add(l1);
		p.add(l2);
		p.add(t1);
		p.add(t2);
		p.add(b1);
		p.add(b2);
		p.add(b3);
		f.add(p);
		f.setVisible(true);
		f.setSize(400,400);
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Login l=new Login();

	}

}
