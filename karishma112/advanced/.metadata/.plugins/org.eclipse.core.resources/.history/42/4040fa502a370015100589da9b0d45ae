package string;

public class StringClass {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="abc";
		System.out.println("string1= "+s);
		System.out.println("string1 memory loc= "+s.hashCode());
		s="xyz";
		System.out.println("string1= "+s);
		System.out.println("string1 memory loc= "+s.hashCode());
	}

}
