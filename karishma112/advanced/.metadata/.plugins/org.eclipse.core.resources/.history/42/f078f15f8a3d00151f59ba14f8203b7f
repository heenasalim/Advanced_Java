package swingPK;
import javax.swing.*;

public class Student {
	
JLabel l1,l2,l3,l4;
JTextField t1;
JList ls;
JButton b1,b2,b3,b4,b5;
JComboBox c;
JRadioButton rb;
JFrame f1;
JPanel p;


	public Student() {
		f1=new JFrame("StudentDetails");
		p=new JPanel();
		l1=new JLabel("Name");
		l2=new JLabel("Qualification");
		t1=new JTextField();
		ls=new JList();
		rb=new JRadioButton();
		c=new JComboBox();
		b1=new JButton(">>");
		b2=new JButton("<<");
		b3=new JButton("Register");
		b4=new JButton("Cancel");
		b5=new JButton("Reset");
		
		
		p.add(f1);
		f1.add(l1);
		f1.add(l2);
		f1.add(t1);
		f1.add(l3);
		f1.add(ls);
		f1.add(l4);
		f1.add(b1);
		f1.add(b2);
		f1.add(b3);
		f1.add(b4);
		f1.add(b5);
		f1.add(rb);
		f1.add(c);
		
	}

	
	public static void main(String[] args) {
		

	}

}
