package Applet;


	import java.awt.*;

	import javax.swing.*;
	import java.applet.*;


	public class applet3  extends  JApplet
	{

		Button b1,b2,b3;
		public void init()
		{
			b1=new Button("Red");
			b2=new Button("Green");
			b3=new Button("Blue");
			add(b1);
			add(b2);
			add(b3);
			b1.addActionListener(this);
			b2.addActionListener(this);
			b3.addActionListener(b3);
			
		}
}
